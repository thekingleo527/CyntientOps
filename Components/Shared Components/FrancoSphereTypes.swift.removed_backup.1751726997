    public let notes: String
    public let timestamp: Date
    
    public init(photos: [Data], notes: String, timestamp: Date) {
        self.photos = photos
        self.notes = notes
        self.timestamp = timestamp
    }
}

public typealias TSTaskEvidence = TaskEvidenceCollection
